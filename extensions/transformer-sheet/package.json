{
  "name": "transformer-sheet",
  "displayName": "Transformer Sheet Operations",
  "description": "Excel spreadsheet support for VS Code",
  "version": "0.0.1",
  "publisher": "vscode",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onSheet:xlsx",
    "onSheet:xls",
    "onSheet:csv"
  ],
  "capabilities": {
    "sheet": true
  },
  "contributes": {
    "commands": [{
      "command": "vscode-sheet.readSheet",
      "title": "Read Spreadsheet"
    }]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^16.x",
    "typescript": "^4.9.4"
  }
}
