{
  "name": "transformer-sheet",
  "displayName": "Transformer Sheet Operations",
  "description": "Excel spreadsheet support for VS Code",
  "version": "0.0.1",
  "publisher": "vscode",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "capabilities": {
    "sheet": true
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^16.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  }
}
